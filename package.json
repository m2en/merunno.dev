{
  "name": "merunno.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write",
    "eslint": "eslint --ignore-path .gitignore \"src/**/*.{js,ts,tsx}\"",
    "eslint:fix": "eslint --ignore-path .gitignore \"src/**/*.{js,ts,tsx}\" --fix",
    "fix": "yarn run format && npm run lint -- --fix",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "1.0.17",
    "@chakra-ui/react": "1.6.12",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "esl": "^10.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "8.0.2",
    "@testing-library/react": "12.1.2",
    "@types/node": "16.11.14",
    "@types/react": "17.0.37",
    "eslint": "^7.12.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  }
}
